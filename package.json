{
    "name": "angular-micro-frontend-by-cli-lesson-one",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:all": "npm run start:app1 & npm run start:app2 & npm run start:app3",
        "start:app1": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --liveReload=false --open=false",
        "start:app2": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve app2-restaurant",
        "start:app3": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve app2-orders",
        "build:app1": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --configuration=production",
        "build:app2": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build app2-restaurant --configuration production --configuration=production",
        "build:app3": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build app2-orders --configuration production --configuration=production",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "analyze": "webpack-bundle-analyzer ./dist/angular-micro-frontend-by-cli-lesson-one/stats.json",
        "analyze-build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/angular-micro-frontend-by-cli-lesson-one/stats.json --port 8889",
        "generate-icons": "svg-to-ts"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.2.5",
        "@angular/cdk": "~13.2.5",
        "@angular/common": "~13.2.5",
        "@angular/compiler": "~13.2.5",
        "@angular/core": "~13.2.5",
        "@angular/forms": "~13.2.5",
        "@angular/platform-browser": "~13.2.5",
        "@angular/platform-browser-dynamic": "~13.2.5",
        "@angular/router": "~13.2.5",
        "@auth0/angular-jwt": "^5.0.1",
        "@ngrx/effects": "^13.0.2",
        "@ngrx/entity": "^13.0.2",
        "@ngrx/store": "^13.0.2",
        "@ngrx/store-devtools": "^13.0.2",
        "@ngxs/devtools-plugin": "^3.6.2-dev.master-eaa5d86",
        "@ngxs/logger-plugin": "^3.6.2-dev.master-eaa5d86",
        "@ngxs/store": "^3.6.2-dev.master-eaa5d86",
        "devextreme": "^20.2",
        "devextreme-angular": "^20.2",
        "devextreme-schematics": "^1.2.2",
        "devextreme-themebuilder": "^20.2",
        "lodash-es": "^4.17.21",
        "moment": "^2.29.1",
        "ngx-quicklink": "^0.2.6",
        "rxjs": "~6.5.5",
        "tailwindcss": "^3.0.23",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "^13.1.0",
        "@angular-devkit/build-angular": "^13.2.5",
        "@angular-eslint/builder": "^12.0.0",
        "@angular-eslint/eslint-plugin": "^12.0.0",
        "@angular-eslint/eslint-plugin-template": "^12.0.0",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "^12.0.0",
        "@angular/cli": "~13.2.5",
        "@angular/compiler-cli": "~13.2.5",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.3.0",
        "@typescript-eslint/parser": "5.3.0",
        "angular-in-memory-web-api": "^0.11.0",
        "eslint": "^8.2.0",
        "faker": "^4.1.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.16",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^13.0.0",
        "protractor": "~7.0.0",
        "svg-to-ts": "^5.7.1",
        "ts-node": "~8.3.0",
        "typescript": "~4.5.5",
        "webpack-bundle-analyzer": "^4.1.0"
    },
    "svg-to-ts": {
        "conversionType": "constants",
        "srcFiles": [
            "./src/assets/icons/svg-icons/**/*.svg"
        ],
        "outputDirectory": "./src/assets/icons",
        "interfaceName": "SvgIcon",
        "typeName": "svgIcon",
        "prefix": "svgIcon",
        "svgoConfig": {
            "plugins": [
                {
                    "cleanupAttrs": true
                }
            ]
        },
        "fileName": "svg-icons.constants",
        "additionalModelFile": "./src/assets/icons",
        "compileSources": true
    }
}
